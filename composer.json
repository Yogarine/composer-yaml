{
  "name": "yogarine/composer-yaml",
  "type": "composer-plugin",
  "description": "composer.yaml support for composer.",
  "license": "MIT",
  "keywords": ["composer", "yaml", "composer.yaml", "composer.yml"],
  "authors": [
    {
      "name": "Alwin Garside",
      "email": "alwin@garsi.de"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "ext-json": "*",
    "composer-plugin-api": ">=1.1",
    "symfony/yaml": ">=2.0"
  },
  "require-dev": {
    "composer/composer": ">=1.1",
    "phpunit/phpunit": "^4.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Yogarine\\Composer\\": "src/"
    }
  },
  "extra": {
    "class": "Yogarine\\Composer\\YamlPlugin"
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
