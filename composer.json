{
    "name": "empaphy/composer-yaml",
    "type": "composer-plugin",
    "description": "composer.yaml support for composer.",
    "license": "MIT",
    "keywords": ["composer", "yaml", "composer.yaml", "composer.yml"],
    "authors": [
        {
            "name": "Alwin Garside",
            "email": "alwin@garsi.de"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "ext-json": "*",
        "composer-plugin-api": ">=1.0",
        "symfony/yaml": ">=2.0"
    },
    "require-dev": {
        "composer/composer": ">=1.0",
        "mikey179/vfsstream": "^1.0",
        "phpunit/phpunit": ">=4.0 <8.0"
    },
    "autoload": {
        "psr-4": {
            "Empaphy\\Composer\\": "src/"
        }
    },
    "extra": {
        "class": "Empaphy\\Composer\\YamlPlugin"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
